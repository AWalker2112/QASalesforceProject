<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_Future_Date_Submitted</fullName>
    <active>true</active>
    <errorConditionFormula>Submitted_Date__c &gt;= TODAY()</errorConditionFormula>
    <errorDisplayField>Submitted_Date__c</errorDisplayField>
    <errorMessage>The date must not be in the future.</errorMessage>
</ValidationRule>
